{% block contact_form %}
    {{ form_start(carouselForm) }}
    <div class="card">
        <div class="col-md-12">
            <div class="card-text">
                {{ form_errors(carouselForm) }}
                {% block carousel_form_rest %}
                    {{ form_rest(carouselForm) }}
                {% endblock %}
            </div>
        </div>
        <div class="card-footer">
            <a href="{{ path('idnkpcarousel_carousel_index') }}" class="btn btn-outline-secondary">
                {{ 'Cancel'|trans({}, 'Admin.Actions') }}
            </a>
            <button class="btn btn-primary float-right">
                {{ 'Save'|trans({}, 'Admin.Actions') }}
            </button>
        </div>
    </div>
    {{ form_end(carouselForm) }}
{% endblock %}